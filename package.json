{
  "name": "fumadocs-template",
  "version": "0.1.0",
  "description": "A modern documentation website template built with Next.js and Fumadocs",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "type": "module",
  "engines": {
    "node": ">=20.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/showlotus/fumadocs-template.git"
  },
  "homepage": "https://github.com/showlotus/fumadocs-template",
  "bugs": {
    "url": "https://github.com/showlotus/fumadocs-template/issues"
  },
  "author": "showlotus",
  "license": "MIT",
  "keywords": [
    "fumadocs",
    "next.js",
    "documentation",
    "mdx",
    "template"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "eslint",
    "test": "vitest"
  },
  "dependencies": {
    "@orama/orama": "^3.1.18",
    "@orama/tokenizers": "^3.1.18",
    "ahooks": "^3.9.6",
    "fumadocs-core": "16.4.2",
    "fumadocs-mdx": "14.2.4",
    "fumadocs-ui": "16.4.2",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@babel/types": "^7.28.5",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "es-module-lexer": "^2.0.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.5.6",
    "raw-loader": "^4.0.2",
    "recast": "^0.23.11",
    "shiki": "^3.20.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
